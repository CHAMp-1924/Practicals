#include <stdio.h>

int main() {
    int n, size;

    printf("Enter the size of the array: ");
    scanf("%d", &size);

    int arr[size];

    printf("Enter %d elements:\n", size);
    for(int i = 0; i < size; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter the position n: ");
    scanf("%d", &n);

    if(n >= 1 && n <= size) {
        printf("The %dth element is: %d\n", n, arr[n-1]);
    } else {
        printf("Invalid position!\n");
    }

    return 0;
}
